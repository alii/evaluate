{
  "name": "scraggy",
  "version": "0.0.9",
  "type": "module",
  "description": "A safe JavaScript expression evaluator",
  "license": "MIT",
  "types": "./dist/index.d.cts",
  "main": "./dist/index.cjs",
  "homepage": "https://github.com/alii/scraggy",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alii/scraggy.git"
  },
  "bugs": {
    "url": "https://github.com/alii/scraggy/issues"
  },
  "keywords": [
    "javascript",
    "evaluator",
    "ast",
    "eval",
    "sandbox",
    "safe-eval"
  ],
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "test": "bun test",
    "build": "tsup src/index.ts --dts --format esm,cjs",
    "release": "bun run build && bun publish"
  },
  "files": [
    "README.md",
    "package.json",
    "dist"
  ],
  "dependencies": {
    "acorn": "^8.11.3"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "tsup": "^8.4.0",
    "typescript": "^5.3.3"
  }
}
